{
  "version": 2,
  "alias": "bdickason.com",
	"builds": [
		{ "src": "_site/**/*.html", "use": "@now/html-minifier" },
    { "src": "static/*", "use": "@now/static"}
  ],
  "routes": [
    { "src": "^/static/(.*)", "dest": "static/$1"},
    { "src": "^/(.*)", "dest": "_site/$1" }
  ],
  "github": {
   "enabled": false
  }
}
